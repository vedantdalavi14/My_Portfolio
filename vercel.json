{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/contact",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
} 